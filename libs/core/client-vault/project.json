{
  "name": "core-client-vault",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/client-vault/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core/client-vault",
        "main": "libs/core/client-vault/src/index.ts",
        "tsConfig": "libs/core/client-vault/tsconfig.lib.json",
        "assets": ["libs/core/client-vault/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/core/client-vault/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/core/client-vault/jest.config.ts"
      }
    }
  },
  "tags": ["layer:core", "type:security"]
}
