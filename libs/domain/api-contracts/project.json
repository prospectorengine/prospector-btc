{
  "name": "api-contracts",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/api-contracts/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/domain/api-contracts",
        "main": "libs/domain/api-contracts/src/index.ts",
        "tsConfig": "libs/domain/api-contracts/tsconfig.lib.json",
        "assets": ["libs/domain/api-contracts/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/domain/api-contracts/**/*.ts"]
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/domain/api-contracts/tsconfig.lib.json --noEmit"
      }
    }
  },
  "tags": ["layer:domain", "type:contract"]
}
